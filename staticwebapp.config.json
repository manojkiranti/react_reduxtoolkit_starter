{
    
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html",
        "statusCode": 200
      }
    },
    "mimeTypes": {
      ".json": "text/json",
      ".woff2": "font/woff2",
      ".ico": "image/x-icon"
    }
  }
  